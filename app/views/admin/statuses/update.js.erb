<% if @status.block? %>
  $("#block-<%= @status.id %>").html("<%= j(render('admin/statuses/unlock_index', status: @status)) %>");
  $("#block_show-<%= @status.id %>").html("<%= j(render('admin/statuses/unlock_show', status: @status)) %>");
<% else %>
  $("#block-<%= @status.id %>").html("<%= j(render('admin/statuses/lock_index', status: @status)) %>");
  $("#block_show-<%= @status.id %>").html("<%= j(render('admin/statuses/lock_show', status: @status)) %>");
<% end %>
